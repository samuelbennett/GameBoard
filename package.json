{
  "name": "game-board",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/lib/umd.js",
  "scripts": {
    "clean": "rimraf ./dist ./temp ./coverage ./coverage-final",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts",
    "tsc": "tsc",
    "umd": "node browserify-umd.js",
    "minify": "node minify-umd.js",
    "build": "npm run lint && npm run tsc && npm run umd && npm run minify",
    "test": "mocha ./dist/test/**/*.js",
    "test-break": "mocha --debug-brk ./dist/test/**/*.js",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/**/*.js && remap-istanbul -i coverage/coverage.json -o coverage-final -t html",
    "all": "npm run clean && npm run build && npm run test && npm run cover",
    "sloc": "sloc src/ && sloc src/ --details --format cli-table --keys total,source,comment"
  },
  "author": "",
  "dependencies": {
    "es6-promise": "^3.0.0",
    "lodash": "^4.0.0",
    "typescript-collections": "^1.1.2"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "browserify": "^13.0.0",
    "browserify-umdify": "^1.0.3",
    "chai": "^3.4.0",
    "glob": "^7.0.0",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.2.10",
    "tslint": "^3.3.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "uglify-js": "^2.6.2",
    "util": "^0.10.3"
  }
}
